{
  "name":"Youtube Ad skipper",
  "version":"0.5",
  "description":"It montiors youtube for 'Skip Ad' button and use the button to skip ads.",
  "permissions":["activeTab"],
  "content_scripts":[{
    "matches":["https://www.youtube.com/*"],
    "js":["adSkipper.js"]
  }],
  "host_permissions": ["https://www.youtube.com/*"],
  "action":{
    "default_title":"Youtube Ad skipper",
    "default_popup":"popup/index.html",
    "default_icon":{
      "128":"adSkipper.png"
    }
  },
  "icons":{
    "128":"adSkipper.png"
  },
  "manifest_version": 3
}